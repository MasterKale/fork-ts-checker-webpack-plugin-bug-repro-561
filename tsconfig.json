{
  "compilerOptions": {
      "sourceMap": true,
      "target": "ES5",
      "lib": ["es2015"],
      "jsx": "react",
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "declaration": false,
      "noImplicitReturns": true,
      "strict": true,
      "baseUrl": "./src/",
      "typeRoots": [
        "./node_modules/@types",
        "./src/types"
      ]
  }
}
